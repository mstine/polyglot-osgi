<ivysettings>
	<resolvers>
		<chain name="bnd-resolver" returnFirst="true">
			<filesystem name="local-cache"><!-- Default local cache -->
				<artifact pattern="${user.home}/.ivy2/cache/[organisation]/[module]/[type]s/[artifact]-[revision].[ext]" />
			</filesystem>
			<ibiblio name="aQute-as-mvn-repo" m2compatible="true" root="http://www.aqute.biz/repo" />
		</chain>
	</resolvers>
	<modules>
		<module resolver="bnd-resolver" />
	</modules>
</ivysettings>

